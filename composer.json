{
    "name": "forecho/laravel-repository",
    "description": "Base repository implementation for Laravel",
    "keywords": [
        "forecho",
        "laravel-repository"
    ],
    "homepage": "https://github.com/forecho/laravel-repository",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "forecho",
            "email": "caizhenghai@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Forecho\\LaravelRepository\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Forecho\\LaravelRepository\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RepositoryServiceProvider"
            ],
            "aliases": {
                "LaravelRepository": "Forecho\\LaravelRepository\\LaravelRepositoryFacade"
            }
        }
    }
}
